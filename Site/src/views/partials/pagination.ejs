<nav aria-label="Page navigation example">
    <ul class="pagination justify-content-end">
      <li class="page-item <%=(pageActive==0)?'disabled':''%>">
        <a class="page-link" href="?page=<%=pageActive%>" aria-label="Previous">
            <span aria-hidden="true">&laquo;</span>
          </a>
      </li>
      <% for (let index = 1; index <= totalPages; index++) { %>
        <li class="page-item <%= (pageActive+1==index)?'active':'' %>" ><a class="page-link" href="?page=<%=index%>"><%=index%></a></li>

          
     <% } %>
      
      <li class="page-item <%=(pageActive+1==totalPages)?'disabled':''%>">
        <a class="page-link" href="?page=<%=pageActive+2%>" aria-label="Next">
            <span aria-hidden="true">&raquo;</span>
          </a>
      </li>
    </ul>
  </nav>